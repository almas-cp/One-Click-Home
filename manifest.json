{
  "manifest_version": 3,
  "name": "One-Click Home",
  "version": "1.0",
  "description": "Click extension icon to navigate to domain root",
  "permissions": [
    "activeTab"
  ],
  "action": {
    "default_title": "Go to domain root",
    "default_icon": "icon.png"
  },
  "icons": {
    "16": "icon.png",
    "32": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "background": {
    "service_worker": "background.js"
  }
}
